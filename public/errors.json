{
    "scenario_title": "sss",
    "scenario_type": "USER",
    "workloads": [
        {
            "service": "*/*_http",
            "executor": "otel",
            "trace_role": "server",
            "protocol": "HTTP",
            "rule": {
                "type": "rule_group",
                "condition": "AND",
                "rules": [
                    {
                        "type": "rule",
                        "id": "\"span_kind\"",
                        "field": "\"span_kind\"",
                        "input": "string",
                        "operator": "equal",
                        "value": "CLIENT",
                        "datatype": "string"
                    }
                ]
            }
        },
        {
            "service": "*/*_http",
            "executor": "ebpf",
            "trace_role": "server",
            "protocol": "HTTP",
            "rule": {
                "type": "rule_group",
                "condition": "AND",
                "rules": [
                    {
                        "type": "rule",
                        "id": "\"attributes\".\"net.host.name\"",
                        "field": "\"attributes\".\"net.host.name\"",
                        "input": "string",
                        "operator": "equal",
                        "value": "ss",
                        "datatype": "string"
                    }
                ]
            }
        }
    ],
    "group_by": [
        {
            "workload_index": 0,
            "title": "\"span_kind\"",
            "hash": "\"span_kind\""
        },
        {
            "workload_index": 1,
            "title": "\"attributes\".\"net.host.name\"",
            "hash": "\"attributes\".\"net.host.name\""
        }
    ],
    "rate_limit": [
        {
            "bucket_max_size": 10,
            "bucket_refill_size": 10,
            "tick_duration": "1m"
        }
    ]
}